{

  "name": "@transmate-eu/carrier-connect",

  "publishConfig": {

    "registry": "https://npm.pkg.github.com"

  },

  "version": "1.0.2",

  "description": "module to connect to carriers",

  "main": "index.js",

  "scripts": {

    "build": "export NODE_ENV=production && webpack --config webpack.config.js",

    "build-test": "export NODE_ENV=testing WEBPACK_TEST=yes && webpack --config webpack.config.js",

    "test": "SANDBOX=true mocha --require @babel/register ./tests --timeout 30000 --exit",

    "test-webpack": "SANDBOX=true DEBUG='*,-babel*,-nodemon*,-mocha*,-follow-redirects*'  WEBPACK_TEST=yes mocha --require @babel/register ./tests --timeout 30000 --exit",

    "start-gql-server": "export DEBUG='*,-babel*,-nodemon*,-express*' && nodemon ./server.js --exec babel-node",

    "gql-function": "export DEBUG='*,-babel*,-nodemon*' && nodemon ./functions/graphql.js --exec babel-node",

    "gql-test": "mocha --require @babel/register ./tests/gql --timeout 30000 --exit",

    "gql-test-webpack": "npm run build-test && WEBPACK_TEST=yes && mocha ./tests/gql --timeout 30000 --exit",

    "start-prod": "node dist/graphql-local.js",

    "deploy": "ibmcloud fn action update dev dist/rest-local.js --web true",

    "deploy-graphql": "ibmcloud fn action update graphql dist/graphql-local.js --web true",

    "start": "nodemon ./index.js --exec babel-node",

    "buildwebpacktest": "npm run build-test && npm run test-webpack"

  },

  "repository": {

    "type": "git",

    "url": "git+https://github.com/Transmate-EU/carrier-connect.git"

  },

  "author": "",

  "license": "ISC",

  "bugs": {

    "url": "https://github.com/Transmate-EU/carrier-connect/issues"

  },

  "homepage": "https://github.com/Transmate-EU/carrier-connect#readme",

  "dependencies": {

    "@babel/cli": "^7.6.4",

    "@babel/node": "^7.6.3",

    "@babel/plugin-syntax-dynamic-import": "^7.2.0",

    "@babel/register": "^7.6.2",

    "@easypost/api": "^3.10.1",

    "@graphql-tools/schema": "^7.1.5",

    "ajv": "^6.12.6",

    "axios": "^0.21.1",

    "debug": "^4.3.1",

    "graphql": "^15.5.0",

    "graphql-tools": "^7.0.5",

    "lodash.get": "^4.4.2",

    "moment": "^2.29.1",

    "nodemon": "^2.0.7",

    "openwhisk": "^3.21.3",

    "pg": "^8.5.1",

    "postmen": "^1.0.4",

    "shippo": "^1.6.0",

    "uuid": "^8.3.2"

  },

  "devDependencies": {

    "@babel/core": "^7.14.3",

    "@babel/plugin-transform-runtime": "^7.14.3",

    "@babel/preset-env": "^7.14.2",

    "apollo-server": "^2.24.0",

    "apollo-server-express": "^2.24.0",

    "babel-eslint": "^10.1.0",

    "babel-loader": "^8.2.2",

    "babel-plugin-import-graphql": "^2.8.1",

    "chai": "^4.3.4",

    "dotenv-json": "^1.0.0",

    "eslint": "^7.27.0",

    "eslint-config-airbnb": "^18.2.1",

    "eslint-config-prettier": "^8.3.0",

    "eslint-plugin-import": "^2.22.1",

    "eslint-plugin-jsx-a11y": "^6.4.1",

    "eslint-plugin-mocha": "^9.0.0",

    "eslint-plugin-no-only-tests": "^2.6.0",

    "eslint-plugin-prettier": "^3.4.0",

    "eslint-plugin-react": "^7.21.5",

    "eslint-plugin-react-hooks": "^4 || ^3 || ^2.3.0 || ^1.7.0",

    "express": "^4.17.1",

    "express-graphql": "^0.12.0",

    "mocha": "^8.3.2",

    "prettier": "2.3.0",

    "webpack": "^5.37.0",

    "webpack-bundle-analyzer": "^4.4.1",

    "webpack-cli": "^4.7.0",

    "webpack-graphql-loader": "^1.0.2"

  }

}

